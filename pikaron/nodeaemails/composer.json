{
	"name": "pikaron/nodeaemails",
	"type": "phpbb-extension",
	"description": "This extension prevents the registration of a user in the forum when using a disposable or temporary mail account (DEA).",
	"homepage": "https://www.phpbb-es.com/foro/viewtopic.php?f=147&t=43035",
	"version": "1.3.0",
	"time": "2019-10-01",
	"keywords": ["phpbb", "extension", "DEA", "email", "disposable"],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Picaron",
			"homepage": "https://github.com/picaronin",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.6",
		"phpbb/phpbb": ">=3.2.4"
	},
	"extra": {
		"display-name": "No DEA Emails",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.4"
		},
		"version-check": {
			"host": "www.siteproall.com",
			"directory": "/pikaron/nodeaemails",
			"filename": "nodeaemails_version"
		}
	}
}
